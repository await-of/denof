{
  "devDependencies": {
    "typescript": "3.9.3",
    "typescript-deno-plugin": "1.29.0"
  },
  "scripts": {
    "format:package": "deno fmt ./deps.ts && deno fmt ./mod.ts && deno fmt ./test.ts",
    "format:source": "deno fmt ./src/**/*.ts",
    "run": "deno run  --allow-all --unstable -c tsconfig.json mod.ts",
    "test": "deno test ./test.ts"
  },
  "type": "module"
}
